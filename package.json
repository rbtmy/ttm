{
  "name": "ttm",
  "version": "0.0.1",
  "description": "Twitter Time Machine",
  "keywords": [
    "twitter",
    "fetch",
    "old",
    "statuses"
  ],
  "main": "server.js",
  "repository": {
    "type": "git"
  },
  "scripts": {
    "start": "NODE_ENV=development ./node_modules/.bin/nodemon --harmony server.js",
    "prod": "npm run build && NODE_ENV=production node --harmony server.js",
    "build": "NODE_ENV=production webpack --config webpack.config.js --separate-stylesheet -p --progress --profile --colors",
    "hot-dev-server": "webpack-dev-server --content-base public -ds --config webpack.config.js --hot --progress --colors --inline",
    "dev-server": "npm run hot-dev-server",
    "test": "npm run base-test && npm run lint",
    "base-test": "NODE_ENV=test ./node_modules/.bin/mocha --harmony --reporter spec ./test/test-*.js",
    "lint": "eslint src app test webpack config lib server.js webpack.config.js"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.12.0",
    "iojs": ">=1.6.0"
  },
  "dependencies": {
    "cheerio": ">=0.20.0",
    "koa": "^2.0.0",
    "jade": "^1.11.0",
    "koa-router": "^7.0.1",
    "koa-static": "^3.0.0",
    "koa-views": "^5.0.1",
    "superagent": "~1.3.0",
    "swig": "1.4.x"
  },
  "devDependencies": {
    "babel": "~5.8.23",
    "babel-core": "~5.8.23",
    "babel-eslint": "~4.1.0",
    "babel-loader": "~5.3.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "~5.8.20",
    "css-loader": "~0.16.0",
    "eslint": "~1.2.1",
    "eslint-plugin-babel": "~2.1.1",
    "eslint-plugin-react": "~3.3.0",
    "extract-text-webpack-plugin": "~0.8.2",
    "file-loader": "~0.8.1",
    "html-loader": "~0.3.0",
    "koa-proxy": "^0.3.0",
    "koa-sess-mongo-store": "~0.1.1",
    "less": "~2.5.0",
    "less-loader": "~2.2.0",
    "mocha": "~2.2.4",
    "null-loader": "~0.1.1",
    "react-hot-loader": "~1.2.9",
    "react-proxy-loader": "~0.3.4",
    "should": "~7.0.4",
    "style-loader": "~0.12.3",
    "supertest": "~1.1.0",
    "url-loader": "~0.5.5",
    "webpack": "~1.12.0",
    "webpack-dev-server": "~1.10.1",
    "yargs": "~3.21.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-async-to-generator": "^6.7.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-register": "^6.7.2",
    "nodemon": "^1.9.1"
  }
}
